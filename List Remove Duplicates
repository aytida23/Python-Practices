#List Remove Duplicates
from __future__ import print_function

a = [1,1,2,2,2,3,4,5,4,5,6,7,8,9,7,8]
b = []

#using loop
def new_list():
    for i in a:
        if i not in b:
            b.append(i)
    return b
print ("Here is the list without duplicate elements using loop:-")
print (new_list())
print()

#using set type
def new_list1():
    b = set(a)
    return list(b)

print()
print ("Here is the list without duplicate elements using set:-")
print (new_list1())

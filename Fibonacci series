#Fibonacci Series
from __future__ import print_function

num = input("Enter how many numbers you want to generate in a fibonacci series :- ")
num1, num2 = 0, 1
print()
print ("Here are the fibonacci series :- ",end="")
for i in range(0,num):
    print (num1,end=" ")
    temp_num1 = num1
    num1 = num2
    num2 = temp_num1 + num2
